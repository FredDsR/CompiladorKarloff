void main () {
	newVar integer times;
	newVar integer output;
	times = System.readint();	
	output = fib(times);
	System.output(output);
}

func integer fib (integer times) {
	newVar integer result;
	newVar integer past;
	result = 0;
	past = 1;
	if ((times == 1)) then {
		return 0;
	};
	times = times - 1;
	while ((times > 1)) {
		result = (result + past);
		past = result;
		times = (times - 1);
	};
	return result;
}
